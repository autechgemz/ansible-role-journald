---

journald_debug: false

journald_config_options:
  Storage: 'auto'
  Compress: 'yes'
  ForwardToConsole: 'no' 
  ForwardToSyslog: 'no'
  MaxRetentionSec: '1month'
  RateLimitBurst: '10000'
  RateLimitIntervalSec: '30s'
  SyncIntervalSec: '1s'
  SystemMaxUse: '8G'
  SystemKeepFree: '20%'
  SystemMaxFileSize: '10'